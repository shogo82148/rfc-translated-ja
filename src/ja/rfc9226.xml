    <title>Bioctal: 16進数 2.0</title>
    <seriesInfo name="RFC" value="9226" stream="independent"/>
    <author initials="M." surname="Breen" fullname="Michael Breen">
      <organization showOnFrontPage="true">mbreen.com</organization>
